    "url": "git+ssh://git@bitbucket.org/ahimta/fuel-consumption-calculator.git"
  },
  "keywords": [
    "fuel",
    "calculator",
    "electricity",
    "gas",
    "gasoline",
    "patrol",
    "price",
    "water"
  ],
  "author": "Abdullah Alansari <ahimta@gmail.com>",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/ahimta/fuel-consumption-calculator#readme",
  "devDependencies": {
    "del": "^2.2.2",
    "gulp": "^3.9.0",
    "gulp-changed": "^1.3.0",
    "gulp-connect": "^2.3.1",
    "gulp-favicons": "^2.2.5",
    "gulp-gh-pages": "^0.5.4",
    "gulp-htmlmin": "^1.3.0",
    "gulp-jade": "^1.1.0",
    "gulp-manifest": "^0.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.1",
    "gulp-usemin": "^0.3.16",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",

  "lint-staged": {
    "{gulpfile.js,README.md,scripts/*.js,app/manifest.json,app/scripts/*.js}": ["prettier --write", "git add"],
    "{gulpfile.js,scripts/*.js,app/scripts/*.js}": ["prettier --write", "standard --fix", "git add"]
  }
